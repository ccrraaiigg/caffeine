{

    "WEEKS" : [
        { "name" : "Sunday",    "nickname" : "Sun", "isWeekend": true, "color": "red" },
        { "name" : "Monday",    "nickname" : "Mon" },
        { "name" : "Tuesday",   "nickname" : "Tue" },
        { "name" : "Wednesday", "nickname" : "Wed" },
        { "name" : "Thursday",  "nickname" : "Thu" },
        { "name" : "Friday",    "nickname" : "Fri" },
        { "name" : "Saturday",  "nickname" : "Sat", "isWeekend": true, "color": "red" }
    ],

    "MONTHS" : [
          { "name" : "January",  "nickname" : "Jan" },
          { "name" : "February", "nickname" : "Feb" },
          { "name" : "March",    "nickname" : "Mar" },
          { "name" : "April",    "nickname" : "Apr" },
          { "name" : "May",      "nickname" : "May" },
          { "name" : "June",     "nickname" : "Jun" },
          { "name" : "July",     "nickname" : "Jul" },
          { "name" : "August",   "nickname" : "Aug" },
          { "name" : "September","nickname" : "Sep" },
          { "name" : "October",  "nickname" : "Oct" },
          { "name" : "November", "nickname" : "Nov" },
          { "name" : "December", "nickname" : "Dec" }
    ],

    "DaysGrid" : {
        "GridCaption": {
            "inheritProperties" :true,
            "Label" : {
                "padding" : [0, 2, 0, 2],
                "font"    : "%{zebkit.ui.boldFont}"
            },

            "border" : null,
            "background2": null,
            "namesOfWeekDays" : [ "%{WEEKS}" ]
        },

        "ItemPan" : {
            "layout" : { "@zebkit.layout.FlowLayout" : [ "right", "center", "horizontal", 2 ] },

            "icon" : {
                "border": null,
                "background": null,
                "view": null,
                "padding": 0,
                "preferredSize": [-1, -1]
            },

            "label" : {
                "color" : "#442244",
                "font"  : { "@zebkit.ui.Font" : [ "Arial", "bold", "14" ] },
                "border":  null,
                "background": null,
                "padding" : 0
            },

            "textDecorations" : {},

            "border": null,
            "background": null
        },

        "tags" : {
            "prevMonth" : {
                "color" : "lightGray"
            },

            "nextMonth" : {
                "color" : "lightGray"
            },

            "today" : {
                "icon" :  {
                    "view"          : null,
                    "border"        : { "@zebkit.ui.RoundBorder" : []},
                    "background"    : "#65B661",
                    "preferredSize" : [ 8, 8 ]
                }
            },

            "notSelectable" : {
                "color": "gray",
                "textDecorations" : {
                    "strike" : "black"
                }
            }
        },

        "paintNetOnCaption" : true,
        "paintPosMarkerOver": false,
        "usePsMetric"       : true,
        "drawSideLines"     : false,
        "drawLines"         : [ false, true ],
        "lineColor2"         : "lightGray",
        "navigationMode"    : "cell",
        "defCellAlignments" : [ "none", "center" ],
        "views": {
            "offmarker"           : "rgba(234, 243, 119, 0.5)",
            "marker"              : "rgba(234, 243, 119, 1.0)",
            "notSelectableMarker" : { "@zebkit.ui.Border": "lightGray" }
        },

        "selDayBackground" : "#D5F377"
    },

    "Calendar" : {
        "LeftArrowButton" : {
            "preferredSize" : [10, 10],
            "id"       : "leftButton"
        },

        "RightArrowButton" : {
            "preferredSize" : [10, 10],
            "id"             : "rightButton",
            "arrowDirection" : "right"
        },

        "TopArrowButton" : {
            "id"             : "topButton",
            "arrowDirection" : "top",
            "preferredSize"  : [ 12, 8 ]
        },

        "BottomArrowButton" : {
            "id"            : "bottomButton",
            "arrowDirection": "bottom",
            "preferredSize"  : [ 12, 8 ]
        },

        "Link" : {
            "id" : "nowLink",
            "font":  { "@zebkit.ui.Font" : ["Arial", 11]},
            "visible" : false
        },

        "DotButton" : {
            "id"         : "dotButton",
            "enabled"    : false,
            "border"     : { "@zebkit.ui.RoundBorder" : [ null, 0] },
            "background" : {
                "out"          : "black",
                "over"         : "red",
                "pressed.over" : "black",
                "disabled"     : "lightGray"
            },
            "preferredSize" : [ 10, 10 ]
        },

        "MonthsCombo" : {
            "Label": {

                "font" : { "@zebkit.ui.Font" : [ "Arial", "bold", "14" ] },
                "padding" : 4,
                "inheritProperties" : true
            },

            "CompList" : {
                "border" : null,
                "padding": 2
            },

            "id"     : "monthsCombo",
            "months" : [ "%{MONTHS}" ],
            "padding": 2,
            "background" : null,

            "inheritProperties" : true,

            "border" : {
                "focusoff" :  null,
                "focuson"  : { "@zebkit.ui.LineView" : [ "bottom", "black", 1 ] }
            }
        },

        "YearField" : {
            "id"    : "yearField",
            "font" : { "@zebkit.ui.Font" : [ "Arial", "bold", "14" ] },
            "editable"   : false,
            "background" : null,
            "curView" : null,
            "color" : "#276733",
            "border" : {
                "focusoff" :  null,
                "focuson"  : { "@zebkit.ui.LineView" : [ "bottom", "black", 1 ] }
            }
        },

        "InfoPan" : {
            "background": "#C0F785",
            "padding" : 2
        },

        "border": "plain",
        "background" : "#ECFFE6"
    },

    "DateTextField" : {
        "editable" : false
    },

    "DateInputField" : {
        "DateTextField" : {
            "border": null,
            "editable" : false
        },

        "Button" : {
            "border": null,
            "padding": 4,
            "canHaveFocus" : false
        },

        "Calendar" : {
            "MonthsCombo" : {
                "inheritProperties" : true
            },

            "inheritProperties" : true
        },

        "border" : { "@zebkit.ui.Border": "gray" },
        "format" : "${date}/${month2}/${fullYear}"
    },

    "DateRangeInput" : {
        "DateInputPan" : {
            "layout" : { "@zebkit.layout.FlowLayout": 2 }
        },

        "MinDateTextField" : {
            "inheritProperties" : true,
            "border"     : null,
            "background" : null,
            "format"     : "${date}/${month2}/${fullYear}"
        },

        "MaxDateTextField" : {
            "inheritProperties" : true,
            "border"       : null,
            "background"   : null,
            "textAlign"    : "right",
            "format"       : "${date}/${month2}/${fullYear}"
        },

        "LeftArrowButton" : {
            "arrowDirection": "left",
            "preferredSize"  : [ 8, 14 ]
        },

        "RightArrowButton" : {
            "arrowDirection" : "right",
            "preferredSize"  : [ 8, 14 ]
        },

        "Line" : {
            "colors"        : [ "black", "gray" ],
            "preferredSize" : [10, -1]
        },

        "tags" : {
            "startDate" : {
                "border" : { "@zebkit.ui.Border": [] }
            },

            "endDate" : {
                "border" : { "@zebkit.ui.Border": [] }
            }
        },

        "border"  : "plain",
        "layout"  : { "@zebkit.layout.FlowLayout" : 2 },
        "padding" : [2,4,2,4]
    }
}